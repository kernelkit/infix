@BOOTIMG@

image aux.ext4 {
	mountpoint = "/aux"
	temporary = true
	size = @AUXSIZE@

	ext4 {
		label = "aux"
	}
}

image cfg.ext4 {
	empty = true
	temporary = true
	size = @CFGSIZE@

	ext4 {
		label = "cfg"
	}
}

image var.ext4 {
	empty = true
	temporary = true
	size = @VARSIZE@

	ext4 {
		label = "var"
	}
}

image @DISKIMG@ {
	size = @TOTALSIZE@
	hdimage {
		partition-table-type = "gpt"
	}

@BOOTPART@

	partition aux {
		offset = @AUXOFFS@
		image = "aux.ext4"
	}

	partition primary {
		image = "rootfs.squashfs"
		size = @IMGSIZE@
	}

	partition secondary {
		image = "rootfs.squashfs"
		size = @IMGSIZE@
	}

	partition cfg {
		image = "cfg.ext4"
	}

	partition var {
		image = "var.ext4"
	}
}

# Silence genimage warnings
config {}
